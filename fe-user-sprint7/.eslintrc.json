{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    // "eslint:recommended",
    "plugin:react/recommended"
    // "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "eqeqeq": "warn",
    "no-var": "warn",
    "quotes": ["error", "single", { "avoidEscape": true }],
    "react/prop-types": "off",
    "prettier/prettier": 0,
    "react/react-in-jsx-scope": "off",
    "semi": ["error", "always"]
  }
}
